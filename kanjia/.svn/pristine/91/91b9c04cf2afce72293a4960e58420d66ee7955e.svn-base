/*订单搜索*/
#search_name_type{height:23px;border:solid 1px #ccc;border-radius:2px;}



.popup{width: 100%;height: 100%;position: absolute;top: 0;}
.bg{position: fixed;top: 0;left: 0;width: 100%;height: 100%;background-color: rgba(0,0,0,0.5);}
.popup .content{width: 800px;margin:auto;position: relative;top: 20%;background-color: #fff;}
.popup .content p{width: 92%;background-color: #06a7e1;padding:10px 4%;color: #fff;font-size: 15px;}
.popup .content img{width: 30%;margin:5% 35% 0 35%;max-height: 400px;}
.popup .content div{width: 100%;text-align: center;padding:10px 0;font-size: 15px;}
.popup .content div span{font-size: 15px;}
.popup .content .arrow{width: 40px;height: 80px;position: absolute;top: 40%;cursor: pointer;}
.popup .content .leftBtn{left: 27%;}
.popup .content .leftBtn:before{content: '';border-style: solid;border-color: #bfbfbf;border-width: 20px 2px 20px 2px;position: absolute;top: 7px;left: 19px;transform: rotate(45deg);}
.popup .content .leftBtn:after{content: '';border-style: solid;border-color: #bfbfbf;border-width: 20px 2px 20px 2px;position: absolute;bottom: 7px;left: 19px;transform: rotate(-45deg);}
.popup .content .rightBtn{right: 27%;}
.popup .content .rightBtn:before{content: '';border-style: solid;border-color: #bfbfbf;border-width: 20px 2px 20px 2px;position: absolute;top: 7px;left: 19px;transform: rotate(-45deg);}
.popup .content .rightBtn:after{content: '';border-style: solid;border-color: #bfbfbf;border-width: 20px 2px 20px 2px;position: absolute;bottom: 7px;left: 19px;transform: rotate(45deg);}
.popup .content .closeBtn{width: 30px;height: 30px;position: absolute;top: 7px;right: 3%;transform:rotate(45deg);cursor: pointer;}
.popup .content .closeBtn:before{content: '';border-style: solid;border-color: #fff;border-width: 15px 1px 15px 1px;position: absolute;top: 0px;left: 14px;}
.popup .content .closeBtn:after{content: '';border-style: solid;border-color: #fff;border-width: 1px 15px 1px 15px;position: absolute;top: 14px;left: 0px;}















