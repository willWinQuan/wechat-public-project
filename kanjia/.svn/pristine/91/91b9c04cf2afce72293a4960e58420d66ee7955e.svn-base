.btn{
  display: inline-block;
  margin-bottom: 2px;
  font-weight: normal;
  line-height: 1.35;
  text-align: center;
  white-space: nowrap;
  vertical-align: middle;
  -ms-touch-action: manipulation;
      touch-action: manipulation;
  cursor: pointer;
  -webkit-user-select: none;
     -moz-user-select: none;
      -ms-user-select: none;
          user-select: none;
  background-image: none;
  border: 1px solid transparent;
  padding: 5px 10px; 
  font-size: 12px;
  border-radius: 3px;  
}

.btn:focus,
.btn:active:focus,
.btn.active:focus,
.btn.focus,
.btn:active.focus,
