
div,ul,li{
	box-sizing:border-box !important;
}

.all{
	width:88%;
	height:100%;
	border:1px solid #ccc;
	margin:2.6rem;
}

.all_top{
	width:100%;
	height:4rem;
	border:none;
	border-bottom:1px solid #ccc;
}

.all_top>ul{
	width:100%;
    height:100%;
    list-style:none;
}

.all_top li{
    float:left;
    font-size:1.6rem;
    text-align:center;
    line-height:4rem;
    width:9rem;
    height:4rem;
    border:none;
    border-right:1px solid #ccc;
}

.all_top li:hover{
	cursor:pointer;
}

div.all_bottom{
    width:100%;
    height:70rem;
    overflow:hidden;
    border:none;
    /* border:1px solid red; */
}

.all_bottom_x{
	width:100%;
    height:70rem;
    border:1px solid #FFEFDB;
    padding:2.8rem;
}

.all_bottom_x_1{
	width:50rem;
	height:3.8rem;
	/* border:1px solid #ccc; */
}

.all_bottom_x_1>ul{
	list-style:none;
	width:100%;
	height:100%;
}

.all_bottom_x_1 li{
	float:left;
	width:10rem;
	height:100%;
	cursor:pointer;
	text-align:center;
	line-height:3.8rem;
	font-size:1.5rem;
	border:1px solid #ccc;
	border-left:none;
}

.all_bottom_x_1 li:first-child{
	border-left:1px solid #ccc;
}

.all_bottom_x_2{
	width:70rem;
	height:50rem;
	overflow:hidden;
	margin-top:2.2rem;
	/* border:1px solid #ccc; */
}

.all_bottom_x_2_x{
	width:100%;
	height:100%;
	border:1px solid #ccc;
}

.all_bottom_x_2_x div:nth-child(1){
	width:100%;
	height:4.4rem;
	border-bottom:1px solid #ccc;
}

input[name=label_name]{
	width:55%;
	height:90%;
	text-indent:.5rem;
	font-size:1.6rem;
	border:none;
	border-right:1px solid #ccc;
}

input[name=identity_label_name]{
	width:55%;
	height:90%;
	text-indent:.5rem;
	font-size:1.6rem;
	border:none;
	border-right:1px solid #ccc;
}

.all_bottom_x_2_xx.b{
	float:right;
	width:40%;
	height:100%;
	padding-right:2rem;
	/* border:1px solid red; */
}

.all_bottom_x_2_xx.identityy{
	float:right;
	width:40%;
	height:100%;
	padding-right:2rem;
	/* border:1px solid red; */
}

.all_bottom_x_2_x div:nth-child(1) li{
	float:right;
	width:6.6rem;
	height:3rem;
	margin-top:0.5rem;
	margin-right:1rem;
	border:1px solid #ccc;
	font-size:1.5rem;
	text-align:center;
	line-height:3rem;
}

.all_bottom_x_2_x div:nth-child(1) li,.labels_id:hover{
	cursor:pointer;
}

.style_labels{
	width:100%;
	height:100%;
}

.style_labels ul{
	width:100%;
	height:100%;
	padding:2rem;
	padding-top:1.2rem;
}

.style_labels li{
	padding:.5rem .2rem .2rem .5rem;
	height:3.4rem;
	border:1px solid #ccc;
	float:left;
	margin-left:1rem;
	font-size:1.4rem;
	text-align:center;
	line-height:2.4rem;
	margin-top:.8rem;
}

.tb1 tr,.tb1 td,.tb1 span,.tb2 tr,.tb2 td,.tb2 span{
	font-size:1.4rem !important;
	color:#363636;
}

input[name=btn]{
    width:6rem;
    height:2.4rem;
    margin-left:0;
    border:0;
    padding:0;
    background:rgb(25,158,216);
    color:#fff;
    font-size:1.4rem;
    cursor:pointer;
}

#bind_identity{
	width:10rem;
    height:2.4rem;
    margin-left:5rem;
    border:0;
    padding:0;
    background:rgb(25,158,216);
    color:#fff;
    font-size:1.4rem;
    cursor:pointer;
}

