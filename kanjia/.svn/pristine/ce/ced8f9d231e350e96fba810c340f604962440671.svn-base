 html,body{padding:0;margin:0;font-family:'微软雅黑',Times New Roman,serif;}
 body{font-size:1.6rem;}

/* CSS Document */
.place{height:40px;background:url('__PUBLIC__/ass/images/righttop.gif') repeat-x;font-size:12px;}
.place span{float:left;margin-left:12px;line-height:40px;font-weight:bold;}
.place ul{margin:0;padding:0;list-style:none;}
.placeul li{
	float:left;
	padding-left:7px;
	padding-right:12px;
	line-height:40px;
	background:url('__PUBLIC__/ass/images/rlist.gif') no-repeat right;
}
.placeul li:last-child{background:none;}
a:hover{color:#00a4ac;text-decoration:none;}
a,a:focus{text-decoration:none;color:#000;outline:none;blr:expression(this.onFocus=this.blur());}

/*error 404*/
/* .error{background:url('__PUBLIC__/ass/images/404.png') no-repeat;width:490px;margin-top:75px;padding-top:65px;} */
.error{background:url('__PUBLIC__/ass/images/404.png') no-repeat;width:510px;margin-top:75px;padding-top:65px;}
.error h2{margin:0;padding:0;font-weight:normal;font-size:22px;padding-left:154px;}
.error p{margin:0;padding:0;padding-left:154px; line-height:35px;color:#6E6E6E;font-size:12px;}
.reindex{padding-left:154px;}
.reindex a{
	width:115px; 
	height:35px;
	display:block; 
	line-height:35px;
	text-align:center;
	margin-top:8px;
	border-radius:3px;
	font-weight:bold;
	font-size:14px;
	color:rgb(255,255,255);
	background:rgb(60,149,200);
	behavior:url(js/pie.htc);
}

