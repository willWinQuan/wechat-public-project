body{
	background: #fff;
}
.order_detail_h{
	background: url(image/order.png) 50% 50% no-repeat;
    padding: .4rem 12%;
}
.order_detail_obligation{
	color: #eee;
    font-size: .28rem;
}
.order_detail_obligationTime span{
